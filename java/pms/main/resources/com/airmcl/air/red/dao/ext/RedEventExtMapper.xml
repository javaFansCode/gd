<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.airmcl.air.red.dao.ext.RedEventExtMapper" >
  <update id="updateRedEventByRedId" parameterType="java.lang.Integer">
      UPDATE mdd_api_red_event SET state =0 where (red_id=#{redId,jdbcType=INTEGER} and state =1)
  </update>
</mapper>